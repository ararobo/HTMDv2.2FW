{
    "version": 3,
    "configurePresets": [
        {
            "name": "base",
            "hidden": true,
            "binaryDir": "${sourceDir}/build/${presetName}"
        },
        {
            "name": "f303-base",
            "hidden": true,
            "inherits": "base",
            "toolchainFile": "${sourceDir}/targets/HTMDv2.2c-f303/cmake/gcc-arm-none-eabi.cmake",
            "cacheVariables": {
                "TARGET_BOARD": "f303"
            }
        },
        {
            "name": "g431-base",
            "hidden": true,
            "inherits": "base",
            "toolchainFile": "${sourceDir}/targets/HTMDv2.2c-g431/cmake/gcc-arm-none-eabi.cmake",
            "cacheVariables": {
                "TARGET_BOARD": "g431"
            }
        },
        {
            "name": "f303-debug",
            "inherits": "f303-base",
            "displayName": "f303 Debug",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Debug"
            }
        },
        {
            "name": "f303-release",
            "inherits": "f303-base",
            "displayName": "f303 Release",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Release"
            }
        },
        {
            "name": "g431-debug",
            "inherits": "g431-base",
            "displayName": "g431 Debug",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Debug"
            }
        },
        {
            "name": "g431-release",
            "inherits": "g431-base",
            "displayName": "g431 Release",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Release"
            }
        }
    ],
    "buildPresets": [
        {
            "name": "f303-debug",
            "configurePreset": "f303-debug"
        },
        {
            "name": "f303-release",
            "configurePreset": "f303-release"
        },
        {
            "name": "g431-debug",
            "configurePreset": "g431-debug"
        },
        {
            "name": "g431-release",
            "configurePreset": "g431-release"
        }
    ]
}
